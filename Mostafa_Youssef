mkdir VCS.2
git init
git remote add ----
vim Mostafa_Youssef
git add Mostafa_Youssef
git commit -m "Steps File added"
git branch -M main
git push origin main

git branch dev
git branch test
git checkout dev
touch file1 file2
git add .
git commit -m "dev branch has two files add"
git push origin dev
git push origin test

git checkout main
git merge dev
git merge test
git push origin main

remove remotely:
git push origin :dev
git push origin :test

remove localy:
 git branch -D test
 git branch -D dev

git tag -a VCS.300 -m"create annoted tag"
git push origin VCS.300
git tag

delete remote:
git push origin --delete VCS.300
delete local:
git tag -d VCS.300

vim README.md
git add README.md
git commit -m "readme"
git push origin main
